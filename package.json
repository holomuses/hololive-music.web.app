{
	"name": "holomuses",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"start": "sirv build --port 3000",
		"clean": "rimraf .svelte-kit build",
		"fetch:local": "node -r dotenv/config scripts/fetchdata",
		"fetch-dummy:local": "pnpm fetch:local -- dummy.toml",
		"fetch:ci": "node scripts/fetchdata",
		"fetch-dummy:ci": "pnpm fetch:ci -- dummy.toml",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@iarna/toml": "^2.2.5",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/iarna__toml": "^2.0.2",
		"@types/youtube-player": "^5.5.4",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"colorthief": "^2.3.2",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"isomorphic-fetch": "^3.0.0",
		"prettier": "^2.4.0",
		"prettier-plugin-svelte": "^2.4.0",
		"rimraf": "^3.0.2",
		"sass": "^1.39.2",
		"sirv-cli": "^1.0.14",
		"svelte": "^3.42.4",
		"svelte-check": "^2.2.5",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"firebase": "^9.0.2",
		"googleapis": "^84.0.0",
		"url-regex": "^5.0.0",
		"youtube-player": "^5.5.2"
	}
}
